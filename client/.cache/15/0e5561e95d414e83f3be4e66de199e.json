     moviesToshow = moviesToshow.filter(movie => movie.Title.toLowerCase().includes(visibilityFilter.toLowerCase()));\n    }\n\n    if (visibilityFilter !== '' && sortCoulmn === 'Genre') {\n        moviesToshow = moviesToshow.filter(movie.Genre.Name.toLowerCase().includes(visibilityFilter.toLowerCase()));\n    }\n\n    if (visibilityFilter !== '' && sortCoulmn === 'Director') {\n        moviesToshow = moviesToshow.filter(movie.Director.Name.toLowerCase().includes(visibilityFilter.toLowerCase()));\n    }\n\n    return { movies: moviesToshow };\n};\n\nfunction MoviesList(props) {\n    const { movies } = props;\n    \n\n    if (!movies) return <div className=\"main-view\"/>;\n\n    return <div className=\"movie-list\">\n    <VisibilityFilterInput />\n    <Container>\n      <Row>\n      {movies.map(movie => (\n        <Col key={movie._id} xs={6} sm={6} md={4}>\n          <MovieCard key={movie._id} movie={movie} />\n        </Col>\n      ))\n      }\n      </Row>\n    </Container>\n  </div>;\n}\n\nexport default connect(mapStateToProps)(MoviesList);"},"lineCount":null}},"error":null,"hash":"4b5e8950d19982372934129945308227","cacheData":{"env":{}}}