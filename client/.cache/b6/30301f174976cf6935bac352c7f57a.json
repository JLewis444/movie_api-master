:{"line":94,"column":37},"source":"components/login-view/login-view.jsx","original":{"line":63,"column":16}},{"name":"isRequired","generated":{"line":94,"column":38},"source":"components/login-view/login-view.jsx","original":{"line":63,"column":31}},{"generated":{"line":95,"column":0},"source":"components/login-view/login-view.jsx","original":{"line":62,"column":22}},{"generated":{"line":95,"column":1},"source":"components/login-view/login-view.jsx","original":{"line":62,"column":0}}],"sources":{"components/login-view/login-view.jsx":"import React, { useState } from 'react';\nimport axios from 'axios';\nimport PropTypes from 'prop-types';\nimport Button from 'react-bootstrap/Button';\nimport Form from 'react-bootstrap/Form';\n\nimport { Link } from 'react-router-dom';\n\nimport './login-view.scss';\n\nexport function LoginView(props) {\n    const [ username, setUsername ] = useState('');\n    const [ password, setPassword ] = useState('');\n\n    const handleSubmit = () => {\n        event.preventDefault();\n        axios.post('https://lewis-myflix.herokuapp.com/login', {\n            Username: username,\n            Password: password\n        })\n        .then(response => {\n            const data = response.data;\n            props.onLoggedIn(data);\n        })\n        .catch(event => {\n            alert('no such user: ' + username);\n        });\n    };\n\n    return (\n    <Form className=\"login-view\">\n      <h2>Log In!</h2>\n      <Form.Group controlId=\"formBasicEmail\">\n        <Form.Label >Username</Form.Label>\n        <Form.Control type=\"text\" value={username} onChange={e => setUsername(e.target.value)} placeholder=\"Enter Username\" />\n        <Form.Text className=\"text-muted\">\n        Type your username here.\n        </Form.Text>\n      </Form.Group>\n\n      <Form.Group controlId=\"formBasicPassword\">\n        <Form.Label>Password</Form.Label>\n        <Form.Control type=\"password\" value={password} onChange={e => setPassword(e.target.value)} placeholder=\"Password\" />\n      </Form.Group>\n\n      <Button variant=\"primary\" type=\"button\" onClick={handleSubmit}>\n      LOGIN\n      </Button>\n      <p>Not registered???</p>\n      <p>\n        Have Fun\n        <Link to={'/register'}>\n          <span> HERE</span>\n        </Link>\n      </p>\n    </Form>\n    );\n}\n\n// Defining PropTypes\n\nLoginView.propTypes = {\n    onLoggedIn: PropTypes.func.isRequired\n};\n"},"lineCount":null}},"error":null,"hash":"8f83281cf4c6c45d770a83155e8abd8f","cacheData":{"env":{}}}